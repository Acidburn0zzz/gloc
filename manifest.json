{
    "manifest_version": 2,

    "name": "Gloc - github counter lines of code",
    "description": "Counts the number of lines of code of a project from github project page.",
    "version": "1.0.2",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "icons": {
        "200": "icon.png"
    },

    "permissions": [
        "tabs",
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}
